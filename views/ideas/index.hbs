{{ideas.length}}
<div class="">
    {{#each ideas}}
    <div class="card ">
        <div class="card-body">
            <h5 class="card-title">{{title}}</h5>
            <p class="card-text">{{description}}</p>
            <a href="ideas/{{id}}" class="btn btn-primary">Read more</a>
        </div>
    </div>
    {{/each}}
</div>